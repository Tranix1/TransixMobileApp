{
  "expo": {
    "name": "Transix",
    "slug": "Transix",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/TrnaixxNewBG.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yayapana.TransixNewVersion",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TrnaixxNewBG.jpg",
        "backgroundColor": "#0f0e11"
      },
      "package": "com.yayapana.TransixNewVersion",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.POST_NOTIFICATIONS",
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyACiyh-wyKUcTXP0w9FU_N00l7L1ahZP8w"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-navigation-bar",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/TransixxLogoN.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f0e11",
          "dark": {
            "image": "./assets/images/blackBgWhiteLogo.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#0f0e11"
          }
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": false,
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take photos."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "ndkVersion": "26.1.10909125",
            "extraGradleProperties": {
              "android.enableR8.fullMode": "false",
              "android.enableDexingArtifactTransform.desugaring": "false",
              "android.useAndroidX": "true",
              "android.enableJetifier": "true",
              "android.enable16kbPageSizeSupport": "true",
              "android.enablePageSizeCompatMode": "true",
              "android.bundle.enableUncompressedNativeLibs": "false",
              "android.enableProguardInReleaseBuilds": "true",
              "android.enableR8": "true",
              "android.enableR8.fullMode": "false"
            },
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so",
                "**/libjsc.so"
              ],
              "jniLibs": {
                "useLegacyPackaging": false
              }
            },
            "proguardMinifyEnabled": true,
            "proguardFiles": [
              "proguard-rules.pro"
            ]
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/TrnaixxNewBG.jpg",
          "color": "#6a0c0c",
          "defaultChannel": "myNotificationChannel"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ea0fdbd3-e7bb-48b0-b1f5-b0adde88ef7e"
      }
    },
    "owner": "yayapana"
  }
}